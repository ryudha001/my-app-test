{
  "name": "MyAppTest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "dev": "yarn cp:config:dev && yarn install && npx pod-install",
    "dev:ios": "yarn dev && npx react-native run-ios --simulator='iPhone SE (3rd generation)'",
    "dev:android": "yarn dev && yarn android",
    "reinstall": "rm -rf node_modules && yarn install",
    "reinstall-pods": "cd ios && pod deintegrate && pod cache clean --all && pod install && cd ..",
    "assembleDebug:dev": "yarn cp:config:dev && yarn bundle && cd android && ./gradlew clean && ./gradlew assembleDebug && cd .. && yarn nav:debug",
    "assembleRelease:dev": "yarn cp:config:dev && yarn bundle && cd android && ./gradlew clean && ./gradlew assembleRelease && cd .. && yarn nav:release",
    "assembleRelease": "yarn cp:config:prod && yarn bundle && cd android && ./gradlew clean && ./gradlew assembleRelease && cd .. && yarn nav:release",
    "nav:debug": "cd android/app/build/outputs/apk/debug && open .",
    "nav:release": "cd android/app/build/outputs/apk/release && open .",
    "bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "bundleWithAssets": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "cp:config:dev": "cp -r docs/config/dev/ src/config",
    "cp:config:prod": "cp -r docs/config/prod/ src/config",
    "build-api": "npx orval clean",
    "archive": "yarn cp:config:prod && ./archive.sh",
    "archive:dev": "yarn cp:config:dev && ./archive.sh",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native/new-app-screen": "0.81.4",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.12",
    "@reduxjs/toolkit": "^2.9.0",
    "@rneui/base": "^4.0.0-rc.7",
    "@tanstack/react-query": "^5.90.2",
    "axios": "^1.12.2",
    "eventemitter3": "^5.0.1",
    "jwt-decode": "^4.0.0",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-bootsplash": "^6.3.11",
    "react-native-device-info": "^14.1.1",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-heroicons": "^4.0.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-loading-skeleton": "^1.0.1",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "react-native-mmkv": "^3.3.3",
    "react-native-parsed-text": "^0.0.22",
    "react-native-reanimated": "^3.19.1",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "^15.13.0",
    "react-native-toast-message": "^2.3.3",
    "react-native-vector-icons": "^10.3.0",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.81.4",
    "@react-native/eslint-config": "0.81.4",
    "@react-native/metro-config": "0.81.4",
    "@react-native/typescript-config": "0.81.4",
    "@types/jest": "^29.5.13",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
